\begin_layout Itemize
Unter Benutzung von
\begin_inset Separator latexpar
\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
(\alpha\underline{e}_{1}^{*}+\beta\underline{e}_{2}^{*})^{n}=\left\{ \begin{array}{ll}
(\alpha+\beta)^{n-1}(\alpha\underline{e}_{1}^{*}+\beta\underline{e}_{2}^{*}), & \qquad\alpha\neq-\beta\qquad n=2,3,4,\ldots\\
\underline{0} & \qquad\alpha=-\beta\qquad n=2,3,4\ldots
\end{array}\right.
\]

\end_inset


\end_layout

\begin_layout Standard
erhält man die Elemente von 
\begin_inset Formula $G^{v}$
\end_inset

 für ein beliebiges Element 
\begin_inset Formula $\underline{v}=\alpha\underline{e}_{1}^{*}+\beta\underline{e}_{2}^{*}\in LG$
\end_inset

 explizit durch die Beziehung
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\exp\left(t\underline{v}\right)=\left\{ \begin{array}{ll}
\underline{Id}+\frac{1}{\alpha+\beta}\left(\exp\left(\left(\alpha+\beta\right)t\right)-1\right)\underline{v}, & \qquad\alpha\neq-\beta\\
\underline{Id}+t\underline{v}, & \qquad\alpha=-\beta,
\end{array}\right.
\]

\end_inset


\end_layout

\begin_layout Standard
und als Trajektorien in der Gruppe 
\begin_inset Formula $G^{+}$
\end_inset

 erhält man (siehe auch Abbildung 
\begin_inset CommandInset ref
LatexCommand ref
reference "UnterGruppen"

\end_inset

)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
a\left(\underline{v},t\right)=\left\{ \begin{array}{ll}
\frac{\alpha}{\alpha+\beta}\left(1-\exp\left(\left(\alpha+\beta\right)t\right)\right), & \qquad\alpha\neq-\beta\\
-t\alpha & \qquad\alpha=-\beta.
\end{array}\right.\label{KartenKoordinateA}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
c\left(\underline{v},t\right)=\left\{ \begin{array}{ll}
\frac{\beta}{\alpha+\beta}\left(1-\exp\left(\left(\alpha+\beta\right)t\right)\right), & \qquad\alpha\neq-\beta\\
-t\beta & \qquad\alpha=-\beta.
\end{array}\right.\label{KartenKoordinateC}
\end{equation}

\end_inset


\end_layout

\end_deeper
