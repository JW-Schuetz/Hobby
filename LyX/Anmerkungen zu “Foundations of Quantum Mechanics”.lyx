\begin_layout Standard
Durch Anwendung der Kettenregel 
\begin_inset Formula 
\begin{eqnarray}
\frac{d}{d\phi}\left[f\left(g\left(\phi\right)\right)\right] & = & \frac{df\left(g\right)}{dg}\frac{dg\left(\phi\right)}{d\phi}\label{eq:Kettenregel}
\end{eqnarray}

\end_inset

folgt für die 2.
 Ableitung von 
\begin_inset Formula $\phi\left(t\right)$
\end_inset

 nach der Zeit 
\begin_inset Formula 
\begin{eqnarray*}
\ddot{\phi} & = & \frac{d\dot{\phi}}{dt}\\
 & = & \dot{\frac{d\phi}{d\phi}\frac{d\phi}{dt}}\\
 & = & \dot{\phi}\frac{d\dot{\phi}}{d\phi}.
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Ebenso durch Anwendung der Kettenregel folgt 
\begin_inset Formula 
\begin{eqnarray*}
\frac{d}{d\phi}\left(\frac{1}{2}\dot{\phi}^{2}\right) & = & \frac{d}{d\phi}\left[f\left(g\left(\phi\right)\right)\right]\\
 & = & \dot{\phi}\frac{d\dot{\phi}}{d\phi}.
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
Es gilt also 
\begin_inset Note Note
status collapsed

\begin_layout Subsubsection*
Beispiel 1
\end_layout

\begin_layout Plain Layout
Sei 
\begin_inset Formula 
\begin{eqnarray*}
\phi\left(t\right) & = & \cos\omega t.
\end{eqnarray*}

\end_inset

Dann gilt 
\begin_inset Formula 
\begin{eqnarray*}
\frac{d}{dt}\phi\left(t\right) & = & -\omega\sin\omega t\\
 & = & \mp\omega\sqrt{1-\cos^{2}\omega t}\\
 & = & \mp\omega\sqrt{1-\phi^{2}\left(t\right)}\\
\\
\frac{d^{2}}{d^{2}t}\phi\left(t\right) & = & -\omega^{2}\cos\omega t\\
 & = & -\omega^{2}\phi\left(t\right).
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Plain Layout
Für den Term 
\begin_inset Formula $\frac{d}{d\phi}\left(\frac{1}{2}\dot{\phi}^{2}\right)$
\end_inset

 erhält man wegen 
\begin_inset Formula 
\begin{eqnarray*}
\frac{d\dot{\phi}}{d\phi} & = & \mp\omega\frac{d}{d\phi}\sqrt{1-\phi^{2}\left(t\right)}\\
 & = & \mp\omega\left[1-\phi^{2}\left(t\right)\right]^{-\frac{1}{2}}\left[-2\phi\left(t\right)\right]\\
 & = & \pm2\omega\frac{\phi\left(t\right)}{\sqrt{1-\phi^{2}\left(t\right)}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Plain Layout
den Ausdruck 
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\begin{eqnarray*}
\frac{d}{d\phi}\left(\frac{1}{2}\dot{\phi}^{2}\right) & = & \dot{\phi}\frac{d\dot{\phi}}{d\phi}\\
 & = & -2\omega^{2}\phi\left(t\right)\\
 & = & \frac{d^{2}}{d^{2}t}\phi\left(t\right).
\end{eqnarray*}

\end_inset


\end_layout

\end_inset

 
\begin_inset Note Note
status collapsed

\begin_layout Subsubsection*
Beispiel 2
\end_layout

\begin_layout Plain Layout
Sei 
\begin_inset Formula 
\begin{eqnarray*}
\phi\left(t\right) & = & t^{2}.
\end{eqnarray*}

\end_inset

Dann gilt 
\begin_inset Formula 
\begin{eqnarray*}
\frac{d}{dt}\phi\left(t\right) & = & 2t\\
 & = & 2\sqrt{\phi\left(t\right)}\\
\\
\frac{d^{2}}{d^{2}t}\phi\left(t\right) & = & 2.
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Plain Layout
Für den Term 
\begin_inset Formula $\frac{d}{d\phi}\left(\frac{1}{2}\dot{\phi}^{2}\right)$
\end_inset

 erhält man wegen 
\begin_inset Formula 
\begin{eqnarray*}
\frac{d\dot{\phi}}{d\phi} & = & 2\frac{d}{d\phi}\sqrt{\phi\left(t\right)}\\
 & = & \frac{1}{\sqrt{\phi\left(t\right)}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Plain Layout
den Ausdruck 
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\begin{eqnarray*}
\frac{d}{d\phi}\left(\frac{1}{2}\dot{\phi}^{2}\right) & = & \dot{\phi}\frac{d\dot{\phi}}{d\phi}\\
 & = & 2\sqrt{\phi\left(t\right)}\frac{1}{\sqrt{\phi\left(t\right)}}\\
 & = & 2\\
 & = & \frac{d^{2}}{d^{2}t}\phi\left(t\right).
\end{eqnarray*}

\end_inset


\end_layout

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
\frac{d}{d\phi}\left(\frac{1}{2}\dot{\phi}^{2}\left(t\right)\right) & = & \frac{d^{2}\phi\left(t\right)}{dt^{2}}
\end{eqnarray*}

\end_inset


\end_layout

