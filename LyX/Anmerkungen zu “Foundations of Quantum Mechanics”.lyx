\begin_layout Standard
Integration von 
\begin_inset Formula 
\begin{eqnarray*}
\frac{d}{d\phi}\left(\frac{1}{2}\dot{\phi}^{2}\right)+\omega^{2}\sin\phi\left(t\right) & = & 0
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
über 
\begin_inset Formula $\phi$
\end_inset

 liefert 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\frac{1}{2}\dot{\phi}^{2}-\omega^{2}\cos\phi\left(t\right) & = & C\left(t\right)
\end{eqnarray*}

\end_inset

 
\end_layout

\begin_layout Standard
und nach Multiplikation mit 
\begin_inset Formula $ml$
\end_inset

 folgt mit der Definition von 
\begin_inset Formula $\omega$
\end_inset

 nach Gleichung (
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:Omega"
plural "false"
caps "false"
noprefix "false"

\end_inset

) 
\begin_inset Formula 
\begin{eqnarray*}
E\left(t\right) & = & \frac{ml^{2}}{2}\dot{\phi}^{2}-gml\cos\phi\left(t\right).
\end{eqnarray*}

\end_inset

 
\end_layout

\begin_layout Standard
Differentation nach der Zeit 
\begin_inset Formula $t$
\end_inset

 ergibt 
\begin_inset Formula 
\begin{eqnarray*}
\dot{E}\left(t\right) & = & \frac{ml^{2}}{2}\frac{d}{dt}\left(\dot{\phi}^{2}\right)-gml\frac{d}{dt}\cos\phi\left(t\right)\\
 & = & ml^{2}\dot{\phi}\ddot{\phi}+gml\dot{\phi}\sin\phi\left(t\right)\\
 & = & ml\left[l\ddot{\phi}+g\sin\phi\left(t\right)\right]\dot{\phi}.
\end{eqnarray*}

\end_inset

 
\end_layout

\begin_layout Standard
Einsetzen der Pendelgleichung liefert 
\begin_inset Formula 
\begin{eqnarray*}
\dot{E}\left(t\right) & = & ml\left[-l\omega^{2}\sin\phi\left(t\right)+g\sin\phi\left(t\right)\right]\dot{\phi}\\
 & = & ml\left[g-l\omega^{2}\right]\dot{\phi}\sin\phi\left(t\right)\\
 & = & ml\left[g-l\frac{g}{l}\right]\dot{\phi}\sin\phi\left(t\right)\\
 & = & 0.
\end{eqnarray*}

\end_inset


\end_layout

